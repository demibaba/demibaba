{
  "expo": {
    "name": "토닥토닥",
    "slug": "my-diary-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "my-diary-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "bundleIdentifier": "com.todaktodak.app",
      "supportsTablet": true,
      "newArchEnabled": false,
      "associatedDomains": [
        "applinks:todaktodak.app"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "expo-development-client",
            "CFBundleURLSchemes": [
              "my-diary-app"
            ]
          }
        ]
      }
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F3E9"
    },
    "android": {
      "package": "com.emotiondiary",
      "newArchEnabled": false,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F7F3E9"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "todaktodak.app",
              "pathPrefix": "/join"
            },
            {
              "scheme": "my-diary-app",
              "host": "join"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-font",
      "expo-web-browser",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.232207972245-eqs5voukc84bq1ehumq8kue98v58pap8"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e0d719ef-224d-4331-aefe-e156e7c3d9fc"
      }
    },
    "owner": "dogadogados"
  }
}
